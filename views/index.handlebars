<div class="row">
<div class="col-sm-5 float-left box m-auto">
<h2 class="mx-auto text-dark">Burger Types:</h2>

<ul>
  {{#each burgers}}
    {{#unless devoured}}
      {{> burgers/burger-block devoured=true}}
    {{/unless}}
  {{/each}}
</ul>
</div>



<div class="col-sm-5 float-left box ml-auto">
<h2 class="mx-auto text-dark">Devoured:</h2>

<ul>
  {{#each burgers}}
    {{#if devoured}}
      {{> burgers/burger-block devoured=false}}
    {{/if}}
  {{/each}}
</ul>

</div>
</div>

<div class="row">
  <div class="col-sm-6 ml-auto mt-5 mr-auto">
    <div class="card" style="width: 18rem;">
  <div class="card-body">

<h5 class="card-title">Add a Burger</h3>
<form class="create-form">

  <div class="form-group card-text">
    <label for="ca">Burger Name:</label>
    <input type="text" id="ca" name="name">
  </div>
  <button type="submit" class="btn btn-primary">Add Burger</button>
</form>
  </div>
  </div>
  </div>
  </div>
